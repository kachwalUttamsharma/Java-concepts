package com.hcl.inh;

public class Inhc {

	public static void main(String[] args) {
		new Second();
		Modifier obj2  = new Modifier();
		obj2.a = 30;
		System.out.println(obj2.a);
	}

}
 class First{
	 public First(){
		 System.out.println("base class const.");
	 }
 }
 class Second extends First{
	 public Second(){
		 System.out.println("Derived class const..");
	 }
 }