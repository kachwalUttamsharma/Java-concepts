package com.hcl.day3;

public class Attendance {

	public static void show(String...name){
	for(String s : name){
		System.out.println(s +  "");
	}
	System.out.println();
}
	public static void main(String[] args) {
		show();
		show("Visalakshmi","prem");
		show("vish","daas","dburi","hummm");
		show("laksh");
	}
}