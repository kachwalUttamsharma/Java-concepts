package com.hcl.day3;

class First{
	public void show(){
		System.out.println("First");
	}
}

class Second extends First{
	public void show(){
		super.show();
		System.out.println("Second");
		
	}
}
public class InhDemo {
	public static void main(String[] args) {
		Second obj = new Second();
		obj.show();
		
	}

}
