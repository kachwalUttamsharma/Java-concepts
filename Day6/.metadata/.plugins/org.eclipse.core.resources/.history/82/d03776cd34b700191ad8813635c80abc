package com.hcl.day6.collection;

class Data{
	public void swap(int x,int y){
		int t;
		t=x;
		x=y;
		y=t;
	System.out.println("X value : " +x+ "Y value " +y);
	}
}
public class SwapDemo {
	public static void main(String[] args) {
		Data obj = new Data();
		int x,y;
		x=12;
		y=44;
		obj.swap(x, y);
	}
}
